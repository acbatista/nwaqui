- @specials.order(:order).each do |special|
  - if special.property.present?
    %tr
      %td= special.order
      %td= special.property_id
      %td= special.property.address_name
      %td= special.status ? "sim" : "n√£o"
      %td
        = link_to "", edit_special_path(special), remote: true, class: 'btn btn-success btn-xs glyphicon glyphicon-pencil'
        = link_to "", special_path(special), method: :delete, remote: true, data: { confirm: t(:confirm)},  class: 'btn btn-danger btn-xs glyphicon glyphicon-remove'
